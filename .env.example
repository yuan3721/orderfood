# 环境变量配置模板
# 复制此文件为 .env 并填入实际值

# 数据库
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/orderfood

# JWT 密钥
JWT_SECRET=your-super-secret-jwt-key

# 微信小程序配置 (C端)
WECHAT_APP_ID=your-wechat-app-id
WECHAT_APP_SECRET=your-wechat-app-secret

# 微信支付配置
WECHAT_MCH_ID=your-mch-id
WECHAT_API_KEY=your-api-key
WECHAT_CERT_PATH=/path/to/apiclient_cert.pem
WECHAT_KEY_PATH=/path/to/apiclient_key.pem
WECHAT_NOTIFY_URL=https://your-domain.com/api/v1/payment/notify

# 腾讯云 COS 配置
COS_SECRET_ID=your-cos-secret-id
COS_SECRET_KEY=your-cos-secret-key
COS_BUCKET=your-bucket-name
COS_REGION=ap-guangzhou
